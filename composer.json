{
    "name": "nowdigiverse/google-maps-reviews-widget",
    "description": "A WordPress plugin that displays Google Maps business reviews using browser automation",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "nowdigiverse",
            "email": "info@nowdigiverse.com",
            "homepage": "https://nowdigiverse.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/panther": "^2.0",
        "facebook/webdriver": "^1.15"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Google_Maps_Reviews\\": "includes/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
